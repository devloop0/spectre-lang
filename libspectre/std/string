namespace std {
	namespace string {

		static char* NULL = 0 as char*;

		struct strtok_data_t {
			char* str;
			unsigned int pos;
		}

		func unsigned int strlen(const char* str);

		func int strcmp(const char* str1, const char* str2);

		func char* strcpy(char* str1, const char* str2);
	
		func char* strcat(char* str1, const char* str2);

		func byte* memcpy(byte* dst, const byte* src, unsigned int bytes);

		func byte* memset(byte* dst, const byte val, unsigned int bytes);

		func char* strchr(const char* str, const char c);

		func unsigned int strcspn(const char* str1, const char* str2);

		func int memcmp(const byte* ptr1, const byte* ptr2, unsigned int count);

		func byte* memmove(byte* dst, const byte* src, unsigned int bytes);

		func byte* memchr(const byte* src, const byte val, unsigned int bytes);

		func char* strncat(char* str1, const char* str2, const unsigned int n);

		func const int strncmp(const char* str1, const char* str2, const unsigned int n);

		func char* strncpy(char* str1, const char* str2, const unsigned int n);

		func char* strpbrk(const char* str1, const char* str2);

		func char* strrchr(const char* str1, const char c);

		func const unsigned int strspn(const char* str1, const char* str2);

		func char* strstr(const char* str1, const char* str2);

		func char* strtok(char* str, const char* delim);
	}
}
