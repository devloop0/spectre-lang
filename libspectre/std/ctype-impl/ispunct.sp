import <"std/ctype">

namespace std {
	namespace ctype {

		func bool ispunct(char c) {
			return c == '!' || c == '\"' ||  c == '#' ||
				c == '$' || c == '%' || c == '&' ||
				c == '\'' || c == '(' || c == ')' ||
				c == '*' || c == '+' || c == ',' ||
				c == '-' || c == '.' || c == '/' ||
				c == ':' || c == ';' || c == '<' ||
				c == '=' || c == '>' || c == '?' ||
				c == '@' || c == '[' || c == '\\' ||
				c == ']' || c == '^' || c == '_' ||
				c == '`' || c == '{' || c == '|' ||
				c == '}' || c == '~';
		}
	}
}
